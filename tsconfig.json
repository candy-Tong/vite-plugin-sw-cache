{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "WebWorker",
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "declaration": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "*.ts",
    "vite-plugin-sw-cache/**/*.ts",
    "example/**/*.ts",
    "example/**/*.vue"
  ],
  "compileOnSave": false
}
